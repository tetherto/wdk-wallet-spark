{
  "name": "@wdk/wallet-spark",
  "version": "1.0.0-beta.1",
  "description": "A simple package to manage BIP-32 wallets for the Spark blockchain.",
  "main": "index.js",
  "type": "module",
  "types": "./types",
  "scripts": {
    "build:types": "rimraf *.d.ts **/*.d.ts; tsc",
    "lint": "standard --env jest",
    "lint:fix": "standard --fix --env jest",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "imports": {
    "node:perf_hooks": {
      "bare": "bare-performance",
      "default": "perf_hooks"
    }
  },
  "dependencies": {
    "@buildonspark/bare": "^0.0.7",
    "@buildonspark/spark-sdk": "0.2.10",
    "@scure/bip32": "1.7.0",
    "@wdk/wallet": "https://github.com/tetherto/wdk-wallet.git#develop",
    "bare-assert": "^1.0.2",
    "bare-async-hooks": "^0.0.0",
    "bare-buffer": "^3.3.0",
    "bare-crypto": "^1.11.2",
    "bare-diagnostics-channel": "^1.0.1",
    "bare-encoding": "^1.0.0",
    "bare-events": "^2.6.1",
    "bare-fetch": "^2.5.0",
    "bare-fs": "^4.2.1",
    "bare-http1": "^4.0.4",
    "bare-https": "^2.0.0",
    "bare-module": "^5.0.2",
    "bare-node-diagnostics-channel": "^0.0.1",
    "bare-os": "^3.6.2",
    "bare-path": "^3.0.0",
    "bare-performance": "^1.0.1",
    "bare-process": "^4.2.1",
    "bare-stream": "^2.7.0",
    "bare-tcp": "^2.0.8",
    "bare-tls": "^2.1.2",
    "bare-tty": "^5.0.3",
    "bare-url": "^2.2.2",
    "bare-utils": "^1.5.1",
    "bare-wdk-runtime": "file:../../../bare-wdk-runtime",
    "bare-worker": "^3.0.0",
    "bare-zlib": "^1.3.1",
    "bip39": "3.1.0",
    "event-target-shim": "^6.0.2",
    "sodium-universal": "5.0.1"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "jest": "29.7.0",
    "rimraf": "6.0.1",
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bare": "./bare.js",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  }
}
